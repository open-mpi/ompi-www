SLURM_NODELIST='c6-[3,6-8,12,14,17,22-23],c8-[4,7,9,17,20,28],c15-[5,10,18,20,22-24,28],c16-11'<br class="">
                            SLURM_NODE_ALIASES='(null)'<br class="">
                            SLURM_NPROCS=64<br class="">
                            SLURM_NTASKS=64<br class="">
